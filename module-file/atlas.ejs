<div class="atlas">
    <ul class="atlas-content">
        <% page.posts.forEach(function (v) {%>
            <li class="atlas-content-item">
                <img src="<%= v.src %>"/>
                <p><%= v.title%></p>
            </li>
        <%})
        %>
    </ul>
    <nav class="page-nav atlas-page">
        <%
        var prev_text = theme.nav_text.page_prev;
        var next_text = theme.nav_text.page_next
        %>
        <%- paginator({
            prev_text: prev_text,
            next_text: next_text
        }) %>
    </nav>
</div>
